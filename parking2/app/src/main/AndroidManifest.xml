<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.parking">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".AppCtx"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Notification"></activity>
        <activity android:name=".Remove" />
        <activity android:name=".Add_Replace" />
        <activity android:name=".UpdateSchedule" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Register" />
        <activity android:name=".Profile" />
        <activity android:name=".changePassword" />
        <activity android:name=".changePermit" />
        <activity android:name=".HomePage" />
        <activity android:name=".Recommendations" />
        <activity android:name=".LotInfo" />
        <activity android:name=".Dashboard" />
    </application>

</manifest>